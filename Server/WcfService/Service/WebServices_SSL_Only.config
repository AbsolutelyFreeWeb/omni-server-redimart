<?xml version="1.0" encoding="UTF-8"?>
<services>
  <!-- Use this file when only port 433 is open -->

  <!-- BO BackOfficeService -->
  <service behaviorConfiguration="MgtServiceBehavior" name="LSOmni.Service.BOService">
    <!-- to support SSL also -->
    <endpoint address="" behaviorConfiguration="MgtServiceEndpointBehavior"
      binding="basicHttpBinding" bindingConfiguration="myBasicHttpBindingSSL"
      contract="LSOmni.Service.IBOService" />
  </service>
  <service behaviorConfiguration="MgtServiceBehavior" name="LSOmni.Service.BOJson">
    <!-- to support SSL also -->
    <endpoint address="" behaviorConfiguration="MgtJsonXmlEndpointBehavior"
      binding="webHttpBinding" bindingConfiguration="myWebHttpBindingSSL"
      contract="LSOmni.Service.IBOJson" />
  </service>
  
  <!-- inv/pos apps Service -->
  <!-- to support SSL also -->
  <service behaviorConfiguration="MgtServiceBehavior" name="LSOmni.Service.AppJson">
    <endpoint address="" behaviorConfiguration="MgtJsonXmlEndpointBehavior" binding="webHttpBinding" bindingConfiguration="myWebHttpBindingSSL" contract="LSOmni.Service.IAppJson" />
  </service>

  <!-- UC Service -->
  <service behaviorConfiguration="MgtServiceBehavior" name="LSOmni.Service.UCService">
    <!-- to support SSL also -->
    <endpoint address="" behaviorConfiguration="MgtServiceEndpointBehavior" binding="basicHttpBinding" bindingConfiguration="myBasicHttpBindingSSL" contract="LSOmni.Service.IUCService" />
  </service>
  <!-- to support SSL also -->
    <service behaviorConfiguration="MgtServiceBehavior" name="LSOmni.Service.UCJson">
     <endpoint address="" behaviorConfiguration="MgtJsonXmlEndpointBehavior" binding="webHttpBinding" bindingConfiguration="myWebHttpBindingSSL" contract="LSOmni.Service.IUCJson" />
  </service>
  
  <service behaviorConfiguration="MgtServiceBehavior" name="LSOmni.Service.PortalService">
    <endpoint address="" behaviorConfiguration="MgtServiceEndpointBehavior" binding="basicHttpBinding" bindingConfiguration="myBasicHttpBindingSSL" contract="LSOmni.Service.IPortalService" />
  </service>
</services>